@model IEnumerable<MVCDemoUACS.Models.Employee>
@using MVCDemoUACS.Models;

@{ ViewBag.Title = "Employee List";
}

<div>
    <h2>Employee List</h2>

    <ul>
        @foreach (Employee employee in @Model)
        {
            <li>@Html.ActionLink(employee.FirstName, "Details", new { id = employee.ID })</li>
        }
    </ul>
</div>
@Html.ActionLink("Back to List", "Index", "Department")

