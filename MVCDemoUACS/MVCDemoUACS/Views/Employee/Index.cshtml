@model IEnumerable<MVCDemoUACS.Models.Employee>
@using MVCDemoUACS.Models;

@{ ViewBag.Title = "Employee List";
}

    <div>
        <h2>Employee List</h2>

        @Html.ActionLink("Create new Employee", "Create")

        <ul>
            @foreach (Employee employee in @Model)
            {
            <li>@Html.ActionLink(employee.FirstName, "Details", new { id = employee.ID })&nbsp;&nbsp;@Html.ActionLink("(Edit)", "Edit", new { id = employee.ID })</li>
            }
        </ul>
    </div>
@Html.ActionLink("Back to Department List", "Index", "Department")

