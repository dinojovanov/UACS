@model MVCDemoUACS.Models.Employee
    <table style="font-family:Arial; border:1px solid black; width: 300px">
        <tr>
            <td><b>Full name:</b></td>
            <td>@Model.FirstName&nbsp;@Model.LastName</td>
        </tr>
        <tr>
            <td><b>Gender:</b></td>
            <td>@Model.Gender</td>
        </tr>
        <tr>
            <td><b>Salary:</b></td>
            <td>@Model.Salary</td>
        </tr>
    </table>

